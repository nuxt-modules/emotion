    "lint": "eslint --ext .js,.vue,.ts .",
    "test": "vitest"
  },
  "dependencies": {
    "@emotion/css": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@nuxt/kit": "^3.0.0"
